#!/bin/bash

set -e 

###
# Clear cache
###
if [ -x /usr/sbin/icinga-web-clearcache ]; then
	icinga-web-clearcache
fi

# Remove cache dir
if [ -x /var/lib/icinga-web/app/cache ]; then
	rm -rf /var/lib/icinga-web/app/cache
fi

. /usr/share/debconf/confmodule
. /usr/share/dbconfig-common/dpkg/prerm.mysql
dbc_go icinga-web $@

#DEBHELPER#
